{
	"name": "app",
	"version": "1.0.0",
	"private": true,
	"type": "module",
	"scripts": {
		"check": "run-p -l lint kit:check",
		"lint": "eslint .",
		"kit:check": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json",
		"prepare": "svelte-kit sync",
		"build": "vite build",
		"_build:watch": "vite build -w",
		"_wrangler:dev": "wrangler pages dev .svelte-kit/cloudflare --port 8789",
		"pages:dev": "run-p -l _build:watch _wrangler:dev",
		"clean": "rimraf vite.config.js.timestamp* .svelte-kit",
		"clean:powerwash": "pnpm clean && rimraf node_modules && pnpm install"
	},
	"dependencies": {
		"svelte": "^4.2.10"
	},
	"devDependencies": {
		"@cloudflare/workers-types": "^4.20240129.0",
		"@sveltejs/adapter-cloudflare": "^4.1.0",
		"@sveltejs/kit": "^2.5.0",
		"@sveltejs/vite-plugin-svelte": "^3.0.0",
		"autoprefixer": "^10.4.17",
		"eslint": "^8.56.0",
		"eslint-plugin-svelte": "^2.35.1",
		"npm-run-all": "^4.1.5",
		"postcss": "^8.4.34",
		"rimraf": "^5.0.5",
		"svelte-check": "^3.6.3",
		"typescript": "^5.3.3",
		"vite": "^5.0.12"
	}
}