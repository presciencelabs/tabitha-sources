{
  "name": "app",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "prepare": "svelte-kit sync",
    "check": "pnpm prepare && (pnpm check:svelte & pnpm check:lint)",
    "check:svelte": "svelte-check --tsconfig ./tsconfig.json",
    "check:lint": "eslint .",
    "check:lint:fix": "eslint . --fix",
    "build": "vite build",
    "dev": "vite dev",
    "test:e2e": "playwright test",
    "precommit": "pnpm check && pnpm build && pnpm test:e2e",
    "clean:svelte": "rimraf .svelte-kit",
    "clean:node": "rimraf node_modules",
    "clean:powerwash": "(pnpm clean:svelte & pnpm clean:node) && pnpm install"
  },
  "dependencies": {
    "@sveltejs/kit": "^2.48.3",
    "@tailwindcss/typography": "^0.5.19",
    "daisyui": "^5.3.10",
    "svelte": "^5.43.0",
    "tailwindcss": "^4.1.16"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20251014.0",
    "@eslint/compat": "^1.4.1",
    "@eslint/js": "^9.38.0",
    "@playwright/test": "^1.56.1",
    "@stylistic/eslint-plugin": "^5.5.0",
    "@sveltejs/adapter-cloudflare": "^7.2.4",
    "@sveltejs/vite-plugin-svelte": "^6.2.1",
    "@tailwindcss/vite": "^4.1.16",
    "eslint": "^9.38.0",
    "eslint-plugin-svelte": "^3.12.5",
    "globals": "^16.4.0",
    "postcss": "^8.5.6",
    "rimraf": "^6.0.1",
    "svelte-check": "^4.3.3",
    "svelte-eslint-parser": "^1.4.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.46.2",
    "vite": "^7.1.12",
    "wrangler": "^4.45.2"
  }
}
