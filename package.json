{
  "name": "app",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "prepare": "svelte-kit sync",
    "check": "pnpm prepare && (pnpm check:svelte & pnpm check:lint)",
    "check:svelte": "svelte-check --tsconfig ./jsconfig.json",
    "check:lint": "eslint .",
    "check:lint:fix": "eslint . --fix",
    "build": "vite build",
    "dev": "wrangler pages dev .svelte-kit/cloudflare",
    "test:e2e": "playwright test",
    "precommit": "pnpm check && pnpm build && pnpm test:e2e",
    "clean:svelte": "rimraf .svelte-kit",
    "clean:node": "rimraf node_modules",
    "clean:powerwash": "(pnpm clean:svelte & pnpm clean:node) && pnpm install"
  },
  "dependencies": {
    "@sveltejs/kit": "^2.21.5",
    "@tailwindcss/typography": "^0.5.16",
    "daisyui": "^4.12.24",
    "svelte": "^4.2.20",
    "tailwindcss": "^3.4.17"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20250617.0",
    "@eslint/js": "^9.29.0",
    "@playwright/test": "^1.53.0",
    "@stylistic/eslint-plugin": "^2.13.0",
    "@sveltejs/adapter-cloudflare": "^4.9.0",
    "@sveltejs/vite-plugin-svelte": "^3.1.2",
    "@types/node": "^22.15.32",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.29.0",
    "eslint-plugin-svelte": "^2.46.1",
    "globals": "^15.15.0",
    "postcss": "^8.5.6",
    "rimraf": "^6.0.1",
    "svelte-check": "^4.2.1",
    "svelte-eslint-parser": "^0.43.0",
    "typescript": "^5.8.3",
    "vite": "^5.4.19"
  }
}
